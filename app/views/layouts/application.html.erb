<!DOCTYPE html>
<html>
<head>
  <title>Emacsconf</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% if user_signed_in? %>
  <%= link_to('Sign out', destroy_user_session_path, :method=>'delete') %>
  <% else %>
  <%= link_to('Sign in', new_user_session_path) %>
  <% end %>

  <%= yield %>

</body>
</html>
